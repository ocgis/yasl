  <%=  form_for(selection, remote: true) do |f| %>
  <%=    f.hidden_field :list_id, :value => selection.list.id %>
  <%=    f.check_box(:status, { class: "selectionStatus", id: "selection_" + selection.id.to_s }, "checked", "unchecked") %>
  <%=    f.label selection.id.to_s, selection.item.name %>
  <%   end %>
